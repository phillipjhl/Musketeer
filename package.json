{
    "name": "musketeer",
    "version": "0.2.0",
    "description": "Site with marketing, blog, payments, and contact all in one.",
    "main": "build/musketeer-api/index.js",
    "private": true,
    "engines": {
        "node": ">= 8.16.0",
        "npm": "6.9.0"
    },
    "scripts": {
        "test": "jest",
        "build:client": "webpack",
        "build:client:prod": "webpack --env production --mode production",
        "build:server": "babel ./musketeer-api -d ./build/musketeer-api",
        "build:server:prod": "babel ./musketeer-api -d ./build/musketeer-api",
        "clean": "npm run clean:client && npm run clean:server",
        "clean:client": "rimraf ./build/public",
        "clean:server": "rimraf ./build/musketeer-api",
        "clean:prod": "rimraf ./build",
        "dev": "npm run clean && npm run build:server && concurrently -r -k \"npm run watch:client\" \"npm run watch:server\" \"nodemon ./build/musketeer-api --delay 1\"",
        "start": "npm run clean && npm run build:client && npm run build:server && npm run start:server",
        "start:server": "node ./build/musketeer-api/index.js",
        "watch:client": "webpack --mode development --watch",
        "watch:server": "babel ./musketeer-api -w -d ./build/musketeer-api",
        "build:prod": "npm run clean:prod && npm run build:client:prod && npm run build:server:prod"
    },
    "author": {
        "name": "Phillip Langland"
    },
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "appmetrics": "^5.0.3",
        "appmetrics-dash": "^5.0.0",
        "babel-loader": "^8.0.2",
        "bcrypt": "^5.0.0",
        "bootstrap": "^4.3.1",
        "esm": "^3.2.25",
        "express": "^4.16.3",
        "file-loader": "^2.0.0",
        "isomorphic-fetch": "^3.0.0",
        "mailgun-js": "^0.21.0",
        "morgan": "^1.9.1",
        "mysql": "^2.16.0",
        "passport": "^0.4.0",
        "passport-http-bearer": "^1.0.1",
        "passport-local": "^1.0.0",
        "react": "^16.5.1",
        "react-dom": "^16.5.1",
        "react-router-dom": "^4.3.1",
        "react-stripe-elements": "^2.0.1",
        "rimraf": "^2.6.2",
        "stripe": "^6.12.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.8.3",
        "@babel/register": "^7.8.3",
        "concurrently": "^5.3.0",
        "css-loader": "^1.0.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.0",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.37.0",
        "webpack-cli": "^3.3.6"
    }
}
